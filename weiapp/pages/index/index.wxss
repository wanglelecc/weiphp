button{width: 100%;margin-bottom:20rpx;background: #fff;}
.user-box{
    background:#2fb7f7 url(data:image/png;base64,) no-repeat center bottom;

    padding: 15px 0 0 0;
    background-size: 100% 106rpx;
}
.user-img{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  display: block;
  margin: 0 auto;
}
.user-info{
    background: transparent;
    color:#fff;
    font-size: 14px;
    padding: 10px 0;
    text-align: center;
}
